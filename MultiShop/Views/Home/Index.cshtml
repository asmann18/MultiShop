@model HomeVM
<!-- Carousel Start -->
<div class="container-fluid mb-3">
    <div class="row px-xl-5">
        <div class="col-lg-8">
            <div id="header-carousel" class="carousel slide carousel-fade mb-30 mb-lg-0" data-ride="carousel">
                <ol class="carousel-indicators">
                    @for (int i = 0; i < Model.Sliders.Count; i++)
                    {
                        <li data-target="#header-carousel" data-slide-to="@i" class="@(i==0?"active":"")"></li>
                    }
                </ol>
                <div class="carousel-inner">
                    @for (int i = 0; i < Model.Sliders.Count; i++)
                    {
                        var slider = Model.Sliders[i];

                        <div class="carousel-item position-relative @(i==0?"active":"") " style="height: 430px;">
                            <img class="position-absolute w-100 h-100" src="~/img/@slider.ImagePath" style="object-fit: cover;">
                            <div class="carousel-caption d-flex flex-column align-items-center justify-content-center">
                                <div class="p-3" style="max-width: 700px;">
                                    <h1 class="display-4 text-white mb-3 animate__animated animate__fadeInDown">@slider.Title</h1>
                                    <p class="mx-md-5 px-5 animate__animated animate__bounceIn">@slider.Description</p>
                                    <a class="btn btn-outline-light py-2 px-4 mt-3 animate__animated animate__fadeInUp">@slider.ButtonTitle</a>
                                </div>
                            </div>
                        </div>

                    }

                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="product-offer mb-30" style="height: 200px;">
                <img class="img-fluid" src="~/img/offer-1.jpg" alt="">
                <div class="offer-text">
                    <h6 class="text-white text-uppercase">Save 20%</h6>
                    <h3 class="text-white mb-3">Special Offer</h3>
                    <a class="btn btn-primary">Shop Now</a>
                </div>
            </div>
            <div class="product-offer mb-30" style="height: 200px;">
                <img class="img-fluid" src="~/img/offer-2.jpg" alt="">
                <div class="offer-text">
                    <h6 class="text-white text-uppercase">Save 20%</h6>
                    <h3 class="text-white mb-3">Special Offer</h3>
                    <a class="btn btn-primary">Shop Now</a>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Carousel End -->
<!-- Featured Start -->
<div class="container-fluid pt-5">
    <div class="row px-xl-5 pb-3">
        @foreach (var service in Model.Services)
        {
        <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
            <div class="d-flex align-items-center bg-light mb-4" style="padding: 30px;">
                <h1 class="@service.IconPath text-primary m-0 mr-3"></h1>
                <h5 class="font-weight-semi-bold m-0">@service.Name</h5>
            </div>
        </div>
            
        }
        
    </div>
</div>
<!-- Featured End -->
